  <!-- ONNX Runtime configuration - check local deps folder first, then environment variable -->
  <PropertyGroup>
    <!-- Local deps folder path -->
    <OnnxRuntimeLocalDir>$(ProjectDir)deps\onnxruntime\onnxruntime-win-x64-1.19.2</OnnxRuntimeLocalDir>
    <!-- Check if local ONNX Runtime exists -->
    <HasLocalOnnxRuntime Condition="Exists('$(OnnxRuntimeLocalDir)\include\onnxruntime_cxx_api.h')">true</HasLocalOnnxRuntime>
    <HasLocalOnnxRuntime Condition="!Exists('$(OnnxRuntimeLocalDir)\include\onnxruntime_cxx_api.h')">false</HasLocalOnnxRuntime>
    <!-- Check environment variable as fallback -->
    <HasEnvOnnxRuntime Condition="'$(ONNXRUNTIME_DIR)' != ''">true</HasEnvOnnxRuntime>
    <HasEnvOnnxRuntime Condition="'$(ONNXRUNTIME_DIR)' == ''">false</HasEnvOnnxRuntime>
    <!-- Use local if available, otherwise env variable -->
    <HasOnnxRuntime Condition="'$(HasLocalOnnxRuntime)' == 'true' Or '$(HasEnvOnnxRuntime)' == 'true'">true</HasOnnxRuntime>
    <HasOnnxRuntime Condition="'$(HasLocalOnnxRuntime)' != 'true' And '$(HasEnvOnnxRuntime)' != 'true'">false</HasOnnxRuntime>
    <!-- Resolve actual path -->
    <OnnxRuntimeDir Condition="'$(HasLocalOnnxRuntime)' == 'true'">$(OnnxRuntimeLocalDir)</OnnxRuntimeDir>
    <OnnxRuntimeDir Condition="'$(HasLocalOnnxRuntime)' != 'true' And '$(HasEnvOnnxRuntime)' == 'true'">$(ONNXRUNTIME_DIR)</OnnxRuntimeDir>
    <!-- Configure preprocessor and linker -->
    <OnnxRuntimeDefine Condition="'$(HasOnnxRuntime)' == 'true'">ONNXRUNTIME_AVAILABLE;</OnnxRuntimeDefine>
    <OnnxRuntimeDefine Condition="'$(HasOnnxRuntime)' != 'true'"></OnnxRuntimeDefine>
    <OnnxRuntimeInclude Condition="'$(HasOnnxRuntime)' == 'true'">$(OnnxRuntimeDir)\include;</OnnxRuntimeInclude>
    <OnnxRuntimeInclude Condition="'$(HasOnnxRuntime)' != 'true'"></OnnxRuntimeInclude>
    <OnnxRuntimeLibDir Condition="'$(HasOnnxRuntime)' == 'true'">$(OnnxRuntimeDir)\lib;</OnnxRuntimeLibDir>
    <OnnxRuntimeLibDir Condition="'$(HasOnnxRuntime)' != 'true'"></OnnxRuntimeLibDir>
    <OnnxRuntimeLib Condition="'$(HasOnnxRuntime)' == 'true'">onnxruntime.lib;</OnnxRuntimeLib>
    <OnnxRuntimeLib Condition="'$(HasOnnxRuntime)' != 'true'"></OnnxRuntimeLib>
  </PropertyGroup>
